{
    "hosting": {
        "site": "boitto",
        "public": "dist",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/api/asaas/**",
                "function": "asaasProxy"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    },
    "functions": {
        "source": "functions"
    },
    "storage": {
        "rules": "storage.rules"
    }
}